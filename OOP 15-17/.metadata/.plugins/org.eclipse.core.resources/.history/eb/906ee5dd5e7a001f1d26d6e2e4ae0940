package example1;

public class Person extends Object {
	private String name;
	
	public Person() {
		this.name = "<empty>";
	}
	
	public Person(String name) {
		this();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
}
